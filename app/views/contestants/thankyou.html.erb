<div id="thankyou">
  <div id="content">
    <div class="left">
      <div class="message">
        <div class="arrow_box">
          <p class="text"><%= @contestant_profile.thanks_comment %></p>
        </div>
      </div>
      <div class="picture">
       <%= image_tag @contestant_profile.profile_image.thumb, :width=>'300px;' %>
      </div>
      <% unless logged_in? %>
      <div id="more_vote_btn"><a href="/users/signup" target="_blank"><%= @contestant_profile.name %>さんをもっと応援する？</a></div>
      <% end %>
      <div id="banner_big">
        <script type="text/javascript" style="margin-top:30px;">
        var lists=[
        '<a href="http://pixel-aoyama.com" target="blank_"><%= image_tag 'banner/pixel.jpg' %></a>',
        '<a href="http://pixel-aoyama.com" target="blank_"><%= image_tag 'banner/pixel2.jpg' %></a>',
        '<a href="http://www.surf-bev.com/" target="blank_"><%= image_tag 'banner/surf.jpg' %></a>',
        '<a href="http://www.cosme-ken.org/" target="blank_"><%= image_tag 'banner/cosmeken.jpg' %></a>',
        ];

        var rNo = Math.floor(Math.random() * lists.length);
        $(document).ready(function() {
        document.getElementById('banner_big').innerHTML = lists[rNo];
        });
        </script>
      </div>

    </div>

    <div class="right">
      <div class="buttons">
        <p class="button btn1"><a href="/contestants/group/1">予選第1グループへ</a></p>
        <p class="button btn2"><a href="/contestants/group/2">予選第2グループへ<br><span class="date">11/11(水) スタート！</span></a></p>
        <p class="button btn3 yet"><!-- <a href="/contestants/group/2"> --><span class="group">予選第3グループへ<span><br><span class="date">11/21(土) スタート！</span></p>

      </div>
    </div>

  </div>
</div>
<div id="entry_btn" style="width:300px; margin-bottom:0; margin-top:10px;"><%= link_to "トップページに戻る",'/'  %></div>
